		<script>
			async function actualizarCarrito(codigo,nombre,img,precio){
				var prespuesta=document.getElementById("p-res");
				var cantidad= parseInt(document.getElementById("inputcantidad").value);
				var total= parseFloat(cantidad *  precio);

				try {
                const data= await fetch(`/${codigo}`,{
                    method: 'PUT',
                    headers: {
                    'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({codigo, nombre,img, precio,cantidad,total})//convertimos en una cadena de texto JSON
					
                })
                const res=await data.json();
                if(res.estado){
                    Swal.fire({
                    title: 'Añadido al carrito',
                    icon: 'success',
                    showCancelButton: false,
                    confirmButtonText: 'OK',
                    confirmButtonColor: '#3085d6',
                    }).then((result) => {
                        if (result.value) {

                            window.location.href=`/carrito`;

                        }else if(result.value==undefined){
                            window.location.href=`/carrito`;
                        }
                        return false;
                })
                   
                }else{
                    prespuesta.innerText= res.mensaje;
                }

            } catch (error) {
               console.log("error envio de actualizar carrito"+ error) 
            }
			}
		</script>
		
